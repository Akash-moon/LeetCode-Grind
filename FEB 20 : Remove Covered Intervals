class Solution {
public:
    int removeCoveredIntervals(vector<vector<int>>& intervals) 
    { //left = c, right=d
        int left=-1,right=-1,count=0;
        sort(intervals.begin(),intervals.end());
        
            for(auto it : intervals)
            { if(it[0]>left && it[1]>right)
              { 
                left=it[0]; count++;
                
              }
             
             right = max(right,it[1]);
                
            }
        return count;  
    }
};
